{
    "compilerOptions": {
        "module": "Node16",
        "moduleResolution": "Node16",
        "target": "ES2020",
        "lib": [
            "ES2020",
            "WebWorker"
        ],
        "sourceMap": true,
        "rootDir": "src",
        "outDir": "dist",
        "strict": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedParameters": true
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules",
    ]
}
